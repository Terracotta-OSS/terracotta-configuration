<tccon:tc-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:tccon="http://www.terracotta.org/config" xmlns:foo="http://www.example.com/foo">

  <tccon:services>
    <tccon:service id="foo">
      <foo:foo name="foo"/>
    </tccon:service>
  </tccon:services>
  <tccon:servers>
    <tccon:server>
      <tccon:service-overrides>
        <tccon:service-override overrides="foo">
          <foo:foo name="bar"/>
        </tccon:service-override>
      </tccon:service-overrides>
    </tccon:server>
  </tccon:servers>
</tccon:tc-config>